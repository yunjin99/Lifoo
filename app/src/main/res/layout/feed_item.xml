    <View
        android:id="@+id/mid_line"
        android:layout_width="2dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>


    <com.example.lifoo.src.xmlClass.RoundImageView
        android:id="@+id/feed_item_1"
        android:layout_width="156dp"
        android:layout_height="156dp"
        android:src="@drawable/food_image"
        android:viewportWidth="156"
        android:viewportHeight="156"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toLeftOf="@id/mid_line"
        android:layout_marginRight="16dp"
        android:layout_marginTop="8dp" />

    <TextView
        android:id="@+id/feed_title_1"
        android:layout_width="152dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@id/feed_item_1"
        app:layout_constraintRight_toRightOf="@id/feed_item_1"
        app:layout_constraintTop_toBottomOf="@id/feed_item_1"
        android:layout_marginTop="4dp"
        android:text="밤에는 역시 라면"
        android:fontFamily="@font/noto_sans_cjkkr_regular"
        android:textSize="13sp"
        android:textColor="@color/colorBlack"
        android:gravity="top"
        />

    <TextView
        android:id="@+id/feed_hour_1"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@id/feed_title_1"
        android:layout_marginTop="4dp"
        app:layout_constraintTop_toBottomOf="@id/feed_title_1"
        android:text="1시간 "
        android:fontFamily="@font/noto_sans_cjkkr_regular"
        android:textSize="10sp"
        android:textColor="@color/colorBlack"
        android:gravity="top"
        />

    <TextView
        android:id="@+id/feed_imoji_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintRight_toRightOf="@id/feed_item_1"
